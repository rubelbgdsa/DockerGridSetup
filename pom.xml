public Iterator<EnvironmentPermissions> getPermissionList() {
    List<EnvironmentPermissions> permissionList = new ArrayList<>();

    for (EnvParams params : getPermissionEnv()) {
        String envName = params.getEnvName().toString();
        EnvironmentPermissions permissions = nonProdPermission(envName);
        permissionList.add(permissions);
    }

    return permissionList.iterator();
}
